<?xml version="1.0"?>
<!--
  Same basic setup as multi-consumer, but channel doesn't have a consumer.
-->
<setup xmlns:xi="http://www.w3.org/2001/XInclude">
  <connection>
    <impl>\amqphp\Connection</impl>
    <constr_args>
      <conn_params>
        <vhost k="string">robin</vhost>
        <username k="string">testing</username>
        <userpass k="string">letmein</userpass>
        <heartbeat k="int">5</heartbeat>
        <socketImpl k="string">\amqphp\StreamSocket</socketImpl>
        <socketParams>
          <url k="string">tcp://rabbit2:5672</url>
        </socketParams>
      </conn_params>
    </constr_args>

    <channel>
      <xi:include href="broker-common-setup.xml" xpointer="xpointer(/methods/method)"/>
    </channel>
  </connection>



  <connection>
    <impl>\amqphp\Connection</impl>
    <constr_args>
      <conn_params>
        <vhost k="string">robin</vhost>
        <username k="string">testing</username>
        <userpass k="string">letmein</userpass>
        <heartbeat k="int">5</heartbeat>
        <socketImpl k="string">\amqphp\StreamSocket</socketImpl>
        <socketParams>
          <url k="string">tcp://rabbit1:5672</url>
        </socketParams>
      </conn_params>
    </constr_args>

    <channel>
      <xi:include href="broker-common-setup.xml" xpointer="xpointer(/methods/method)"/>
    </channel>
  </connection>
</setup>