<?xml version="1.0"?>
<setup xmlns:xi="http://www.w3.org/2001/XInclude">
  <connection>
    <impl>\amqphp\Connection</impl>
    <server>
      <vhost k="string">robin</vhost>
      <username k="string">testing</username>
      <userpass k="string">letmein</userpass>
      <heartbeat k="int">5</heartbeat>
      <socketImpl k="string">\amqphp\StreamSocket</socketImpl>
      <socketParams>
        <url k="string">tcp://rabbit2:5672</url>
      </socketParams>
    </server>

    <channel>
      <consumer>
        <impl k="string">DemoConsumer</impl>
        <args>
          <queue k="string">most-basic-q</queue>
        </args>
      </consumer>
      <method>
        <class k="string">basic</class>
        <method k="string">qos</method>
        <args>
          <prefetch-count k="string">1</prefetch-count>
          <global k="boolean">false</global>
        </args>
      </method>
    </channel>

    <xi:include href="broker-common-setup.xml"/>
  </connection>

  <connection>
    <impl>\amqphp\Connection</impl>
